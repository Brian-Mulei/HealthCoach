<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.healthcoach">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/healthcoachlogo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/healthcoachlogo"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".Forms.ExericseForm" />
        <activity android:name=".Forms.FoodForm" />
        <activity android:name=".FoodTrack" />
        <activity android:name=".ExerciseTrack" />

        <activity android:name=".Settings" />
        <activity android:name=".HomePage" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>